{
  "name": "ticketmaster",
  "version": "1.0.0",
  "description": "SDK for the Ticketmaster Open Platform",
  "main": "dist/server/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "build": "webpack && npm run prod",
    "dev": "webpack",
    "prod": "cross-env NODE_ENV=prod webpack",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "postversion": "./scripts/post-release.sh",
    "lint": "eslint ./lib ./test"
  },
  "keywords": [
    "ticketmaster",
    "ticketmaster-api",
    "ticketmaster-sdk",
    "ticketmaster-node",
    "ticketmaster-client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticketmaster-api/sdk-javascript.git"
  },
  "author": "Adam Meghji",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "3.5.0",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "inherits": "^2.0.1",
    "json-loader": "^0.5.4",
    "mocha": "2.5.3",
    "nock": "8.0.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bluebird": "3.4.1",
    "request": "2.74.0"
  },
  "homepage": "https://github.com/ticketmaster-api/sdk-javascript"
}

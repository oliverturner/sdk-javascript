{
  "name": "ticketmaster",
  "version": "1.0.0",
  "description": "SDK for the Ticketmaster Open Platform",
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "build": "npm run prod",
    "dev": "webpack",
    "prod": "cross-env NODE_ENV=production webpack",
    "prod:assess": "cross-env NODE_ENV=production webpack --display-modules --sort-modules-by size",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "postversion": "./scripts/post-release.sh",
    "lint": "eslint ./lib ./test"
  },
  "keywords": [
    "ticketmaster",
    "ticketmaster-api",
    "ticketmaster-sdk",
    "ticketmaster-node",
    "ticketmaster-client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticketmaster-api/sdk-javascript.git"
  },
  "author": "Adam Meghji",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-register": "^6.11.6",
    "chai": "3.5.0",
    "cross-env": "^2.0.0",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "inherits": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "mocha": "3.0.0",
    "nock": "8.0.0",
    "webpack": "^2.1.0-beta.20"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "query-string": "^4.2.2",
    "request": "2.74.0"
  },
  "homepage": "https://github.com/ticketmaster-api/sdk-javascript"
}
